{
  "expo": {
    "name": "Habit Streaks",
    "slug": "habit-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/habit-tracker-logo.png",
    "scheme": "habittracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/habit-tracker-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.luckymbithi.habittracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/habit-tracker-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/habit-tracker-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_APPWRITE_ENDPOINT": "https://fra.cloud.appwrite.io/v1",
      "EXPO_PUBLIC_APPWRITE_PROJECT_ID": "68a2f708000f3165e2e8",
      "EXPO_PUBLIC_APPWRITE_PLATFORM": "com.lucky.habittracker",
      "EXPO_PUBLIC_APPWRITE_DATABASE_ID": "68a31fb20032f6baf1f2",
      "EXPO_PUBLIC_APPWRITE_HABITS_COLLECTION_ID": "68a31ff90014f13c6d6f",
      "EXPO_PUBLIC_APPWRITE_COMPLETIONS_COLLECTION_ID": "68a45ad800383027abe2",
      "router": {},
      "eas": {
        "projectId": "c1210854-e9bc-4ad9-b2ba-aa3d4ce94beb"
      }
    }
  }
}
